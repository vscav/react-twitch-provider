{
  "name": "react-twitch-provider",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "scripts": {
    "build": "tsc",
    "start": "react-scripts start",
    "mock-api": "twitch mock-api start",
    "test": "react-scripts test --testURL=http://localhost:8080",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "yarn prettier:fix && yarn lint:fix",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Vincent Scavinner",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.1",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "^5.0.1",
    "swr": "^1.3.0",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3",
    "zod": "^3.17.10",
    "husky": "^8.0.0"
  }
}